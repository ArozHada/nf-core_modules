nextflow_process {

    name "Test Process TRANSDECODER_LONGORF"
    script "modules/nf-core/transdecoder/longorf/main.nf"
    process "TRANSDECODER_LONGORF"
    config "tests/modules/nf-core/transdecoder_longorf/nextflow.config"
    tag "transdecoder_longorf"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
