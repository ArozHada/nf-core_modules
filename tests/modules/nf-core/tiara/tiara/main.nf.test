nextflow_process {

    name "Test Process TIARA_TIARA"
    script "modules/nf-core/tiara/tiara/main.nf"
    process "TIARA_TIARA"
    config "tests/modules/nf-core/tiara_tiara/nextflow.config"
    tag "tiara_tiara"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
