nextflow_process {

    name "Test Process SHAPEIT5_PHASECOMMON"
    script "modules/nf-core/shapeit5/phasecommon/main.nf"
    process "SHAPEIT5_PHASECOMMON"
    config "tests/modules/nf-core/shapeit5_phasecommon/nextflow.config"
    tag "shapeit5_phasecommon"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            // Assert that the process ran successfully
            assert process.success
            // Assert that the process produced the expected output
            assert snapshot(path(params.outdir).list()).match()
        }

    }

}
