nextflow_process {

    name "Test Process ARTIC_GUPPYPLEX"
    script "modules/nf-core/artic/guppyplex/main.nf"
    process "ARTIC_GUPPYPLEX"
    config "tests/modules/nf-core/artic_guppyplex/nextflow.config"
    tag "artic_guppyplex"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
