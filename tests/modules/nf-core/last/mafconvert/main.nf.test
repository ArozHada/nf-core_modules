nextflow_process {

    name "Test Process LAST_MAFCONVERT"
    script "modules/nf-core/last/mafconvert/main.nf"
    process "LAST_MAFCONVERT"
    config "tests/modules/nf-core/last_mafconvert/nextflow.config"
    tag "last_mafconvert"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
