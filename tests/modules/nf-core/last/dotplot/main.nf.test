nextflow_process {

    name "Test Process LAST_DOTPLOT"
    script "modules/nf-core/last/dotplot/main.nf"
    process "LAST_DOTPLOT"
    config "tests/modules/nf-core/last_dotplot/nextflow.config"
    tag "last_dotplot"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
