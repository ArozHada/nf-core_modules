nextflow_process {

    name "Test Process STADENIOLIB_SCRAMBLE"
    script "modules/nf-core/stadeniolib/scramble/main.nf"
    process "STADENIOLIB_SCRAMBLE"
    config "tests/modules/nf-core/stadeniolib_scramble/nextflow.config"
    tag "stadeniolib_scramble"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
