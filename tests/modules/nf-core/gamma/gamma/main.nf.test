nextflow_process {

    name "Test Process GAMMA_GAMMA"
    script "modules/nf-core/gamma/gamma/main.nf"
    process "GAMMA_GAMMA"
    config "tests/modules/nf-core/gamma_gamma/nextflow.config"
    tag "gamma_gamma"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
