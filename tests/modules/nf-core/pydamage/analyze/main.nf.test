nextflow_process {

    name "Test Process PYDAMAGE_ANALYZE"
    script "modules/nf-core/pydamage/analyze/main.nf"
    process "PYDAMAGE_ANALYZE"
    config "tests/modules/nf-core/pydamage_analyze/nextflow.config"
    tag "pydamage_analyze"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
