nextflow_process {

    name "Test Process SEQWISH_INDUCE"
    script "modules/nf-core/seqwish/induce/main.nf"
    process "SEQWISH_INDUCE"
    config "tests/modules/nf-core/seqwish_induce/nextflow.config"
    tag "seqwish_induce"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
