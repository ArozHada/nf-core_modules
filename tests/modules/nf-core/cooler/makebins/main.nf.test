nextflow_process {

    name "Test Process COOLER_MAKEBINS"
    script "modules/nf-core/cooler/makebins/main.nf"
    process "COOLER_MAKEBINS"
    config "tests/modules/nf-core/cooler_makebins/nextflow.config"
    tag "cooler_makebins"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
