nextflow_process {

    name "Test Process IMPUTEME_VCFTOPRS"
    script "modules/nf-core/imputeme/vcftoprs/main.nf"
    process "IMPUTEME_VCFTOPRS"
    config "tests/modules/nf-core/imputeme_vcftoprs/nextflow.config"
    tag "imputeme_vcftoprs"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
