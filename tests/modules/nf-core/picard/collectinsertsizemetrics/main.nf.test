nextflow_process {

    name "Test Process PICARD_COLLECTINSERTSIZEMETRICS"
    script "modules/nf-core/picard/collectinsertsizemetrics/main.nf"
    process "PICARD_COLLECTINSERTSIZEMETRICS"
    config "tests/modules/nf-core/picard_collectinsertsizemetrics/nextflow.config"
    tag "picard_collectinsertsizemetrics"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
