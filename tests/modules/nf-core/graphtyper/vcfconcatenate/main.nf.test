    process "GRAPHTYPER_VCFCONCATENATE"
    config "tests/modules/nf-core/graphtyper_vcfconcatenate/nextflow.config"
    tag "graphtyper_vcfconcatenate"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
