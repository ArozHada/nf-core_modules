nextflow_process {

    name "Test Process MAXQUANT_LFQ"
    script "modules/nf-core/maxquant/lfq/main.nf"
    process "MAXQUANT_LFQ"
    config "tests/modules/nf-core/maxquant_lfq/nextflow.config"
    tag "maxquant_lfq"

    test("Should run without failures") {

        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                // define inputs of the process here. Example:
                // input[0] = file("test-file.txt")
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(path(params.outdir).list()).match() }
            )
        }

    }

}
